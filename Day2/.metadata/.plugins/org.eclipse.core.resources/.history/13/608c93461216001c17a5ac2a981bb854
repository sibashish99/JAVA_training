package com.example.demo;

public class while_DoWhile {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
         for(int i=0;i<5;i++) {
        	 System.out.println(i+" Hello");
         }
         System.out.println("-------------");
         int j=5;
         do {
        	 System.out.println(j+" world");
        	 j--;
         }while(j>=0);
          
         
         int k=0;
         
         while(k<=5) {
        	 System.out.println(k+" Lumen");
        	 k++;
         }
         
         
	}

}
