package com.exercise.demo;


public class ItemService {
	   public int assignGrade(Item item) {
		   int discount=5;
		   return discount;
	   }
	   
	   public int assignGrade(Item item,String customer) {
		   int discount=10;
		   if(customer.equals("corporate")) {
			   discount=15;
		   }
		   return discount;
	   }
}
